{
  "name": "@felipemelz-liber/spa-poc",
  "version": "0.1.4",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "repository": "git://github.com/felipemelz-liber/spa-poc.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "build": "rollup -c",
